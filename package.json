    "psl": "^1.9.0",
    "qrcode": "^1.5.4",
    "qs": "^6.11.2",
    "tailwindcss": "^3.4.17",
    "typescript-cookie": "^1.0.3",
    "uuid": "^8.3.2",
    "vue": "^3.5.17",
    "vue-chartjs": "^5.3.3",
    "vue-codemirror": "^6.1.1",
    "vue-i18n": "^11.1.11",
    "vue-qrcode": "^2.2.0",
    "vue-router": "^4.5.1",
